# -*- conf-unix -*-
[user]
	name = Tusooa Zhu
	email = tusooa@vista.aero
    signingkey = tusooa@gmail.com
[color]
    ui = always
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[gui]
    fontui = -family ZhunYuan -size 13 -weight normal -slant roman -underline 0 -overstrike 0
    fontdiff = -family Monaco -size 13 -weight normal -slant roman -underline 0 -overstrike 0
[pretty]
    graph = %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset
[alias]
    c = commit -a
    p = push origin HEAD
    s = status
    a = !git.perl
    clog = log --color --graph --pretty=graph --abbrev-commit --
    d = diff
    u = pull
[core]
    autocrlf = false
    safecrlf = false
    quotepath = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[url "git://anongit.kde.org/"]
    insteadOf = kde:
[url "git@git.kde.org:"]
    pushInsteadOf = kde:
